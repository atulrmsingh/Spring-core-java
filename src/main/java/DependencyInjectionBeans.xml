<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="a" class="com.spring.dependencyinjection.DependencyInjection">
   <constructor-arg value="Atul"></constructor-arg>
   <constructor-arg value="26"  type="int"></constructor-arg>
  </bean>

  <bean id="b" class="com.spring.dependencyinjection.DependencyInjectionUsingSetter">
   <property name = "Name" value="atul"/>
   <property name = "Age" value="26"/>
  </bean>

</beans>
